      "root": "libs/api",
      "sourceRoot": "libs/api/src",
      "projectType": "library",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/api/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/api"]
        }
      },
      "prefix": "app"
    },
